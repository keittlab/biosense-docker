[Unit]
Description=Modem Setup Service
After=ModemManager.service NetworkManager.service network-online.target
Wants=network-online.target
Requires=ModemManager.service NetworkManager.service

[Service]
Type=oneshot
ExecStart=/usr/local/bin/setup-modem
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target
